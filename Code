#include <stdio.h>

void main()
{
    int arr[6]={2,5,8,4,19,10};
    int i,k, swap;
    
    for(k=0;k<5;k++)
    {
    for(i=0;i<5;i++)
    {
        if(arr[i]> arr[i+1])
        {
            swap= arr[i];
            arr[i]=arr[i+1];
            arr[i+1]= swap;
        }
    }
    
    printf("%d iteration :  \n",k+1);
    
    for(int g=0;g<6;g++)
    {
    printf(" %d \n",arr[g]);
    }
    }
     printf("Final Array:  \n");
        
        for(int j=0 ; j<6; j++)
        {
            printf("%d \n", arr[j]);
        }
    }
